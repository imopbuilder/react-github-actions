{
  "name": "react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test:dev": "node -r dotenv/config ./node_modules/.bin/vite.ps1",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "pnpm biome format --write ./src",
    "lint": "pnpm biome lint ./src --apply",
    "check": "pnpm biome check ./src --apply",
    "vault:pull": "npx dotenv-vault pull development .env.development.local",
    "vault:push": "npx dotenv-vault push development .env.development.local"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-router": "^1.33.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@tanstack/router-vite-plugin": "^1.32.17",
    "@types/node": "^22.1.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
